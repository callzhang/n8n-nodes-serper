{
  "name": "@callzhang/n8n-nodes-serper",
  "version": "1.0.0",
  "description": "n8n custom node for Serper web search API",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "serper",
    "search",
    "web-search",
    "ai-agent"
  ],
  "license": "MIT",
  "homepage": "https://github.com/callzhang/n8n-nodes-serper",
  "author": {
    "name": "callzhang",
    "email": "callzhang@example.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/callzhang/n8n-nodes-serper.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -s"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/SerperApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Serper/Serper.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.16.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.42.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
